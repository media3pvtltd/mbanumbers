							<th id="th" colspan="3">Member Name</th>
							<th id="th" colspan="3">School Name</th>
							<th id ="th"colspan="2">GMAT</th>
							<th id ="th"colspan="2">GPA</th>
							<th id ="th"colspan="2">Last Updated</th>
						</tr>

			<% unless @updates.nil? %>
				
			
							
			<% @updates.each do |update| %>
							<tr>
						<td colspan="3" style="padding:10px">
							<%= Member.find(update.user_id).username  %>
						</td> 

												
						<td colspan="3" style="padding:10px"><%a= update.school.split('-') %>
							<%= a[0] %>- <%=a[1] %>
						</td>	



				
			<td colspan="2" style="padding:10px;color:#453232;" >

					<%= Member.find(update.user_id).gmat_score  %>
			 </td>		
			<td colspan="2" style="padding:10px;color:#453232;" > 
				
				<%= number_with_precision( Member.find(update.user_id).gpa , precision: 2)  %>
			</td>	
			<td colspan="2" style="padding:10px;color:#453232;" > 

				<%= update.updated_at.strftime("%d-%m-%Y") %>
				
			</td>					
						
							</tr>
			<% end %>
			<% else %>

