    <%=form_tag profile_searchschool_path,:class => 'gdlr-lms-form', :method => 'get' do |f| %>
	     <%= text_field_tag 'schoolname' , "",:style => 'color:black; background-color:white; border:1px solid;border-color: #D7D1D1;border-radius: 4px;',:placeholder => 'Enter School Name', :id => 'tags', :class => 'gdlr-lms-form'  %></div>
